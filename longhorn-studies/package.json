{
  "name": "longhorn-studies",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start --dev-client",
    "web": "expo start --web",
    "ios": "expo run:ios",
    "android": "expo run:android",
    "build:dev": "eas build --profile development",
    "build:preview": "eas build --profile preview",
    "build:prod": "eas build --profile production",
    "prebuild": "expo prebuild",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "gen:types": "supabase gen types typescript --local > supabase/functions/new-spot/types/database.ts && supazod -i supabase/functions/new-spot/types/database.ts -o supabase/functions/new-spot/types/schemas.ts -t supabase/functions/new-spot/types/schemas_infer.d.ts"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@hookform/resolvers": "^5.0.1",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-google-signin/google-signin": "^13.2.0",
    "@react-navigation/native": "^7.1.6",
    "@shopify/flash-list": "^1.7.6",
    "@supabase/supabase-js": "2.49.5-next.5",
    "expo": "^53.0.9",
    "expo-blur": "~14.1.4",
    "expo-clipboard": "~7.1.4",
    "expo-constants": "~17.1.6",
    "expo-dev-client": "~5.1.8",
    "expo-image-picker": "^16.1.4",
    "expo-linear-gradient": "~14.1.4",
    "expo-linking": "~7.1.5",
    "expo-maps": "^0.10.0",
    "expo-media-library": "^17.1.6",
    "expo-router": "~5.0.7",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.7",
    "expo-web-browser": "~14.1.6",
    "nativewind": "latest",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.55.0",
    "react-native": "0.79.2",
    "react-native-draggable-flatlist": "^4.0.2",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "3.17.5",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.10.0",
    "react-native-shimmer-placeholder": "^2.0.9",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "~0.20.0",
    "zod": "^3.24.2",
    "zustand": "^4.5.6"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@types/react": "~19.0.14",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "eslint-config-universe": "^12.1.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "supabase": "^2.20.12",
    "supazod": "^1.2.3",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3"
  },
  "eslintConfig": {
    "extends": "universe/native",
    "root": true
  },
  "private": true,
  "pnpm": {
    "onlyBuiltDependencies": [
      "supabase"
    ]
  }
}